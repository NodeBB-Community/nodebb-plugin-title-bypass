{
  "name": "nodebb-plugin-title-bypass",
  "version": "1.0.0",
  "description": "A plugin allowing the title to be optional",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodebb/nodebb-plugin-title-bypass"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "title-bypass",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodebb/nodebb-plugin-title-bypass/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.1",
    "@commitlint/config-angular": "17.4.0",
    "eslint": "8.31.0",
    "eslint-config-nodebb": "0.2.1",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0"
  }
}
